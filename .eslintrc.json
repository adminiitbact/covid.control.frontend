{
  "extends": "react-app",
  "parser": "babel-eslint",
  "globals": {
    "window": false
  },
  "rules": {
    "no-unused-vars": "off",
    "no-useless-constructor": 0,
    "comma-dangle": [
      1,
      "never"
    ],
    "jsx-quotes": [
      2,
      "prefer-single"
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  }
}