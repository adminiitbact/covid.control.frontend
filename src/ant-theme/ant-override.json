{
  "@primary-color": "#007aff",
  "@font-family": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Lato, 'Helvetica Neue', Arial,'Noto Sans', sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol', 'Noto Color Emoji'",
  "@text-color": "fade(@black, 80%)",
  "@border-radius-base": "0px",
  "@height-base": "48px",
  "@height-lg": "52px",
  "@height-sm": "44px",
  "@btn-primary-shadow": "0 1px 5px 0 rgba(0, 0, 0, 0.15)",
  "@box-shadow-base": "0 1px 5px 0 rgba(0, 0, 0, 0.15)",
  "@btn-font-size-lg": "@font-size-base",
  "@btn-font-size-sm": "@font-size-base",
  "@btn-font-weight": "500",
  "@table-header-color": "rgba(0, 0, 0, 0.8)",
  "@table-padding-vertical": "8px",
  "@table-header-bg": "#fff",
  "@btn-disable-color": "fff",
  "@btn-disable-bg": "#aaaab6",
  "@btn-disable-border": "#aaaab6"
}
